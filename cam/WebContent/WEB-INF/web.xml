	</session-config>

	<mime-mapping>
		<extension>xml</extension>
		<mime-type>text/xml</mime-type>
	</mime-mapping>

	<!-- 救灾图片读取 -->
	<servlet>
		<servlet-name>ShowPicServlet</servlet-name>
