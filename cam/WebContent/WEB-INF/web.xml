<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
	<display-name>loushang5</display-name>

	<context-param>
		<param-name>assemblytype</param-name>
		<param-value>xml</param-value>
	</context-param>

	<filter>
		<filter-name>Cache-Control</filter-name>
		<filter-class>org.loushang.next.web.CacheControl</filter-class>	
		<init-param>
			<param-name>active</param-name>
			<param-value>false</param-value>
		</init-param>		
	</filter>

	<!-- session管理-->
	<filter>
		<filter-name>SecurityContextIntegrationFilter</filter-name>
		<filter-class>
			org.loushang.bsp.security.context.HttpSessionContextIntegrationFilter
		</filter-class>
	</filter>
	<!-- 认证过滤器 -->
	<filter>
		<filter-name>AuthenticationFilter</filter-name>
		<filter-class>
			org.loushang.bsp.security.authen.web.AuthenticationProcessingFilter
		</filter-class>
	</filter>
	<!--过滤请求的SAML请求
	<filter>
		<filter-name>IdPSessionFilter</filter-name>
		<filter-class>
			org.loushang.bsp.security.idp.web.filters.IdPSessionFilter
		</filter-class>
	</filter>
	-->
	<filter>
		<filter-name>CamsExtUserFilter</filter-name>
		<filter-class>com.inspur.cams.comm.extuser.util.CamsExtUserFilter</filter-class>
	</filter>
	<!-- 访问控制器 -->
	<filter>
		<filter-name>AuthorizationFilter</filter-name>
		<filter-class>
			org.loushang.bsp.security.author.intercept.web.FilterSecurityInterceptor
		</filter-class>
	</filter>

	<filter-mapping>
		<filter-name>Cache-Control</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>

	<filter-mapping>
		<filter-name>SecurityContextIntegrationFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>

	<filter-mapping>
		<filter-name>AuthenticationFilter</filter-name>
		<url-pattern>/j_bsp_security_check/*</url-pattern>
	</filter-mapping>
    <!-- 过滤saml请求的过滤器
	<filter-mapping>
		<filter-name>IdPSessionFilter</filter-name>
		<url-pattern>/SAML2/*</url-pattern>
	</filter-mapping>
    -->
    <filter-mapping>
		<filter-name>CamsExtUserFilter</filter-name>
		<url-pattern>*.jsp</url-pattern>
		<dispatcher>REQUEST</dispatcher>
	</filter-mapping>
	<filter-mapping>
		<filter-name>AuthorizationFilter</filter-name>
		<url-pattern>*.jsp</url-pattern>
		<dispatcher>REQUEST</dispatcher>
	</filter-mapping>
	<filter-mapping>
		<filter-name>CamsExtUserFilter</filter-name>
		<url-pattern>/command/ajax/*</url-pattern>
		<dispatcher>REQUEST</dispatcher>
	</filter-mapping>
	<filter-mapping>
		<filter-name>AuthorizationFilter</filter-name>
		<url-pattern>/command/ajax/*</url-pattern>
		<dispatcher>REQUEST</dispatcher>
	</filter-mapping>

	<listener>
		<listener-class>
			org.loushang.sca.ContextLoaderListener
		</listener-class>
	</listener>
	<listener>
		<listener-class>
			org.loushang.bsp.security.web.listener.SessonListener
		</listener-class>
	</listener>

	
	  <!--workflow listener start-->
	  <listener>
        <listener-class>flex.messaging.HttpFlexSession</listener-class>
      </listener>
      <!--workflow listener end-->

	  <!-- live listener start-->
	  <listener>
		<listener-class>
		 org.loushang.live.engine.servlet.JMSInitListener
		</listener-class>
	  </listener>
	  <listener>
		<listener-class>
		 org.loushang.live.engine.servlet.DBTimerInitListener
		</listener-class>
	  </listener>
		<!-- live listener end-->
    <!-- 照片读取开始 -->
    <servlet>
		<servlet-name>CrjDownloadPhoto</servlet-name>
        <servlet-class>com.inspur.sdmz.comm.util.CrjDownloadPhoto</servlet-class>
        <load-on-startup>1</load-on-startup>
	</servlet>
	<!-- 照片读取结束 -->
	<!-- 外部单位登录servlet-->
	<servlet>
		<servlet-name>CamsExtUserLoginServlet</servlet-name>
		<servlet-class>com.inspur.cams.comm.extuser.util.CamsExtLoginServlet</servlet-class>
	</servlet>
	<!-- 照片读取MAPPING开始 -->
	<servlet-mapping>
        <servlet-name>CrjDownloadPhoto</servlet-name>
        <url-pattern>/downloadPhoto</url-pattern>
	</servlet-mapping>
	<!-- 照片读取MAPPING结束 -->
	<servlet-mapping>
		<servlet-name>CamsExtUserLoginServlet</servlet-name>
		<url-pattern>/camsext</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>CamsExtUserLogoutServlet</servlet-name>
		<servlet-class>com.inspur.cams.comm.extuser.util.CamsExtLogoutServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CamsExtUserLogoutServlet</servlet-name>
		<url-pattern>/camsext_logout</url-pattern>
	</servlet-mapping>
	
	  <!--workflow servlet start-->
     <servlet>
        <servlet-name>MessageBrokerServlet</servlet-name>
        <display-name>MessageBrokerServlet</display-name>
        <servlet-class>flex.messaging.MessageBrokerServlet</servlet-class>
        <init-param>
            <param-name>services.configuration.file</param-name>
            <param-value>/WEB-INF/flex/services-config.xml</param-value>
       </init-param>
        <load-on-startup>1</load-on-startup>
    </servlet>
	<servlet>
      <servlet-name>WorkflowServiceServlet</servlet-name>
      <display-name>WorkflowService Servlet</display-name>
      <servlet-class>org.loushang.workflow.webservice.servlet.WorkflowServiceServlet</servlet-class>
    </servlet>
	<!--workflow servlet end-->

	<servlet>
		<servlet-name>logout</servlet-name>
		<servlet-class>
			org.loushang.bsp.security.web.servlet.LogOut
		</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>scaenginer</servlet-name>
		<servlet-class>
			org.loushang.sca.impl.ws.axis.ScaWebServiceServlet
		</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>feed</servlet-name>
		<servlet-class>
			org.loushang.next.common.feed.FeedReOutServlet
		</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>scajsonrpc</servlet-name>
		<servlet-class>
			org.loushang.sca.impl.rpc.JSONRPCServiceServlet
		</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>command_ajax</servlet-name>
		<servlet-class>
			org.loushang.next.web.cmd.CommandServlet
		</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>command_dispatcher</servlet-name>
		<servlet-class>
			org.loushang.next.web.cmd.DispatcherServlet
		</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>chart</servlet-name>
		<servlet-class>
			org.loushang.next.chart.data.ChartServlet
		</servlet-class>
	</servlet>


	<servlet>
		<servlet-name>download</servlet-name>
		<servlet-class>
			org.loushang.next.common.download.DownLoadServlet
		</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>bptdownload</servlet-name>
		<servlet-class>
			com.inspur.cams.comm.servlet.DownLoadServlet
		</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>fileDownload</servlet-name>
		<servlet-class>
			com.inspur.cams.comm.servlet.FileDownLoadServlet
		</servlet-class>
	</servlet>
	<!-- BPT 证书下载servlet -->
	<servlet>
		<servlet-name>bptCertDownload</servlet-name>
		<servlet-class>
			com.inspur.cams.comm.servlet.BptCertDownLoadServlet
		</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>jsondraw</servlet-name>
		<servlet-class>
			org.loushang.demo.draw.JsonManager
		</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AuthenticationServlet</servlet-name>
		<servlet-class>
			org.loushang.bsp.security.web.servlet.AuthenticationServlet
		</servlet-class>
	</servlet>
	
   <servlet>
		<servlet-name>RegistorListener</servlet-name>
		<servlet-class>
			org.loushang.bsp.security.web.listener.RegistorListener
		</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>

	<servlet>
		<servlet-name>MessageSender</servlet-name>
		<display-name>MessageSender</display-name>
		<servlet-class>
		org.loushang.live.engine.servlet.MessageSenderServlet</servlet-class>
	</servlet>

	<!-- saml请求的servlet
  	<servlet>
	    <servlet-name>profilesRequestDispatcherServlet</servlet-name>
		<servlet-class>
			org.loushang.bsp.security.idp.web.servlets.ProfilesRequestDispatcherServlet
		</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>profilesRequestDispatcherServlet</servlet-name>
		<url-pattern>/SAML2/*</url-pattern>
		</servlet-mapping>
	-->
	

<!--axis2-->

	<servlet>
		<servlet-name>WsServlet</servlet-name>
		<servlet-class>
			org.loushang.ws.transport.http.AxisServlet
		</servlet-class>		
		<load-on-startup>1</load-on-startup>
	</servlet>
    <servlet>
        <servlet-name>WsAdminServlet</servlet-name>
        <display-name>Apache-Axis AxisAdmin Servlet (Web Admin)</display-name>
        <servlet-class>
            org.loushang.ws.webapp.AxisAdminServlet</servlet-class>
    </servlet>

<!--file upload begin-->
	 <servlet>
        <servlet-name>UploadServlet</servlet-name>        
        <servlet-class>
            org.loushang.cms.upload.UploadFileServlet</servlet-class>
    </servlet>
<!--file upload end-->
	
    <!--workflow servlet mapping start-->
    <servlet-mapping>
        <servlet-name>MessageBrokerServlet</servlet-name>
        <url-pattern>/messagebroker/*</url-pattern>
    </servlet-mapping>
	<servlet-mapping>
      <servlet-name>WorkflowServiceServlet</servlet-name>
      <url-pattern>/WorkflowServices/*</url-pattern>
    </servlet-mapping>
   <!--workflow servlet mapping end-->

	<servlet-mapping>
		<servlet-name>WsServlet</servlet-name>
		<url-pattern>/servlet/AxisServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>WsServlet</servlet-name>
		<url-pattern>*.jws</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>WsServlet</servlet-name>
		<url-pattern>/services/*</url-pattern>
	</servlet-mapping>

    <servlet-mapping>
        <servlet-name>WsAdminServlet</servlet-name>
        <url-pattern>/ws-admin/*</url-pattern>
    </servlet-mapping>

<!--axis2-->
 	<servlet-mapping>
