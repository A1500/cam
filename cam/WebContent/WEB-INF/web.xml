            com.inspur.cams.dis.base.servlet.ShowPicServlet
        </servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>ShowPicServlet</servlet-name>
		<url-pattern>/showPicServlet</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
