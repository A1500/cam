<?xml version="1.0" encoding="UTF-8"?>
<composite name="MenuConfigSearch" xmlns="http://www.loushang.com/sca/1.0">
    <description>MenuConfigSearch</description>   
    
    <component name="MenuConfigSearchDao">
	    <service name="MenuConfigSearchDao">
	    	<interface.java interface="com.inspur.cams.comm.menuconfig.dao.IMenuConfigSearchDao"/>
        	<binding.sca/>
        </service>
        <property name="dataSourceConf" value="dataSource"/>
        <implementation.java impl="com.inspur.cams.comm.menuconfig.dao.MenuConfigSearchDao"/>	
    </component>
    <component name="pubMenuConfigDomain">
		<service name="pubMenuConfigDomain">
			<interface.java interface="com.inspur.cams.comm.menuconfig.domain.IPubMenuConfigDomain"/>
			<binding.sca/>
