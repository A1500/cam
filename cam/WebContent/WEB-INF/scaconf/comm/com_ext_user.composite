	</component>
	<component name="ComExtUserLoginService">
		<service name="ComExtUserLoginService">
			<interface.java interface="com.inspur.cams.comm.extuser.webservice.IComExtUserLoginService"/>
			<binding.sca/>
			<binding.ws/>
		</service>
		<implementation.java impl="com.inspur.cams.comm.extuser.webservice.ComExtUserLoginService"/>
	</component>
</composite>