<?xml version="1.0" encoding="UTF-8"?>
<composite name="common" xmlns="http://www.loushang.com/sca/1.0">
	<!--农业产业化填报工作信息表-->
	<component name="aimReportWorkDao">
		<service name="aimReportWorkDao">
			<interface.java interface="com.inspur.aims.work.base.dao.IAimReportWorkDao"/>
			<binding.sca/>
		</service>
		<property name="dataSourceConf" value="dataSource"/>
		<implementation.java impl="com.inspur.aims.work.base.dao.jdbc.AimReportWorkDao"/>
    </component>
	<component name="aimReportWorkDomain">
		<service name="aimReportWorkDomain">
			<interface.java interface="com.inspur.aims.work.base.domain.IAimReportWorkDomain"/>
			<binding.sca/>
		</service>
		<implementation.java impl="com.inspur.aims.work.base.domain.support.AimReportWorkDomain"/>
		<reference name="aimReportWorkDao" target="aimReportWorkDao/aimReportWorkDao"/>
	</component>
</composite>