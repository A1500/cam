			<interface.java interface="com.inspur.sdmz.apply.dao.ISamApplyDao"/>
			<binding.sca/>
	    </service>
		<property name="dataSourceConf" value="dataSource"/>
		<implementation.java impl="com.inspur.sdmz.apply.dao.jdbc.SamApplyDao"/>	
	</component>
	<component name="SamApplyHandleDao">
		<service name="ISamApplyHandleDao">
			<interface.java interface="com.inspur.sdmz.apply.dao.ISamApplyHandleDao"/>
