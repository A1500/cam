<script type="text/javascript">
var webObj;
var activeDocument;
var marksNames;

/**
本方法 以实例的方式，约定说明模板、标签和数据的格式
**/
function init(){  
	//取得文件名 和标签    
	var nowFileName = "/jsp/ext/somsd/manage/printWord/fund/doc/branchChange.doc";
	//marksNames = "name1__1&code1__1";  标签定义格式 字段代码 + __ + 序号 
	//打开文件
	webObj = document.getElementById("webOfficeNt");
	webObj.Titlebar = false;
	//webObj.OpenLocalFile(nowFileName);
	webObj.OpenFromURL(L5.webPath+nowFileName);
	activeDocument = webObj.activeDocument;
	
	activeDocument.Application.ActiveWindow.View.ShowBookmarks = true;
	var showMap=new L5.Map("java.util.HashMap");
	
	var somOrganDataSet=L5.DatasetMgr.lookup("somOrganDataSet");
		somOrganDataSet.setParameter("SORG_ID@=", sorgId);
		somOrganDataSet.load();
		somOrganDataSet.on('load', function(){
		
		showMap.put("mainSorgName",somOrganDataSet.get("mainSorgName"));
		showMap.put("mainSorgCode",somOrganDataSet.get("mainSorgCode"));
		showMap.put("cnName",somOrganDataSet.get("cnName"));
		showMap.put("sorgCode",somOrganDataSet.get("sorgCode"));
		
		var somChangeList=L5.DatasetMgr.lookup("somChangeList");
		somChangeList.setParameter("SORG_ID@=", sorgId);
		somChangeList.setParameter("ID@=", id);
		somChangeList.load();
		
		somChangeList.on('load', function(){
		var records=somChangeList.getAllRecords();
		for(var i=0;i<records.length;i++){
			var record =records[i];
			changeItem = record.get("changeItem");
			changeBefore = record.get("changeBefore");
			changeAfter = record.get("changeAfter");
			changeReason = record.get("changeReason");
		
			if(changeItem==0){
		
			showMap.put("changeAfter",changeAfter);
			showMap.put("changeBefore",changeBefore);
			showMap.put("changeReason",changeReason);
			showMap.put("changeItem","变更名称");
			
			}
			else if(changeItem==1){
			showMap.put("changeBefore",changeBefore);
			showMap.put("changeAfter",changeAfter);
			showMap.put("changeReason",changeReason);
		 	showMap.put("changeItem","变更住所");
		 	}
		 	else if(changeItem==2){
		 	showMap.put("changeBefore",changeBefore);
			showMap.put("changeAfter",changeAfter);
			showMap.put("changeReason",changeReason);
			showMap.put("changeItem","变更负责人");
			//需要打印负责人信息
			var url=L5.webPath+"/jsp/ext/somsd/manage/printWord/fund/jsp/branchChange/legalChange.jsp?taskCode="+taskCode+"&sorgId="+sorgId+"&dutyId="+dutyId;
			window.open(url);
			}
			else if(changeItem==3){
			showMap.put("changeBefore",changeBefore);
			showMap.put("changeAfter",changeAfter);
			showMap.put("changeReason",changeReason);
			showMap.put("changeItem","变更业务范围");
			
			}
		}
		getMarks();
		writeDoc(showMap);
		
	});
});
	
}
  
function writeDoc(mp){
	//遍历map的值
	for(var key in mp.map)
	{
		//var getMapMark = mp.get(key);
		//分割标签
		var marks=marksNames.split("&");
		//遍历所有标签
		for(var i=0;i<marks.length;i++)
		{
				//分离标签和标记
				var marksStr = marks[i].split("__");
				//标签和预含义标签相等的场合
				if(marksStr[0]==key)
				{
					//获取标签值
		  			var bkmkObj = activeDocument.Bookmarks(marks[i]);
					//向模板中写数据
					if(bkmkObj)
					{
						var inputValue = mp.get(key);						
						var saverange = bkmkObj.Range;
						saverange.Text = inputValue;
						//saverange.InsertBefore(""); //之前写内容
						//saverange.InsertAfter("");  //之后写内容
					}		  			

			    }
		}
	
	}
}
/**
获得 模板中的标签并组成串 "name1__1&code1__1";  标签定义格式 字段代码 + __ + 序号 
*/        
function getMarks(){

	marksNames = "";
	var count = activeDocument.Bookmarks.Count;
	for(var i=1;i<=count;i++)
	{
		if(i<count){
			marksNames = marksNames + activeDocument.Bookmarks(i).Name + "&";
		}
		else{
			marksNames = marksNames + activeDocument.Bookmarks(i).Name;
		}
	}
	//alert(marksNames);
}




function  returnButton(){
